# 1 "arch/arm/boot/dts/overlays/audremap-overlay.dts"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/audremap-overlay.dts"
/dts-v1/;
/plugin/;

/ {
        compatible = "brcm,bcm2708";

        fragment@0 {
                target = <&audio_pins>;
                frag0: __overlay__ {
                        brcm,pins = < 12 13 >;
                        brcm,function = < 4 >;
                };
        };

 __overrides__ {
  swap_lr = <&frag0>, "swap_lr?";
  enable_jack = <&frag0>, "enable_jack?";
 };
};
