# 1 "arch/arm/boot/dts/overlays/pi3-miniuart-bt-overlay.dts"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/pi3-miniuart-bt-overlay.dts"
/dts-v1/;
/plugin/;
# 18 "arch/arm/boot/dts/overlays/pi3-miniuart-bt-overlay.dts"
/{
 compatible = "brcm,bcm2708";

 fragment@0 {
  target = <&uart0>;
  __overlay__ {
   pinctrl-names = "default";
   pinctrl-0 = <&uart0_pins>;
   status = "okay";
  };
 };

 fragment@1 {
  target = <&uart1>;
  __overlay__ {
   pinctrl-names = "default";
   pinctrl-0 = <&uart1_pins &bt_pins>;
   status = "okay";
  };
 };

 fragment@2 {
  target = <&uart0_pins>;
  __overlay__ {
   brcm,pins;
   brcm,function;
   brcm,pull;
  };
 };

 fragment@3 {
  target = <&uart1_pins>;
  __overlay__ {
   brcm,pins = <32 33>;
   brcm,function = <2>;
   brcm,pull = <0 2>;
  };
 };

 fragment@4 {
  target-path = "/aliases";
  __overlay__ {
   serial0 = "/soc/uart@7e201000";
   serial1 = "/soc/uart@7e215040";
  };
 };
};
